{
    "version": 2,
    "public": false,
    "routes": [
        {
            "src": "/dist/.*",
            "dest": "/dist/server.js"
        },
        {
            "src": "^/(.*)",
            "dest": "/$1"
        },
        {
            "src": ".*",
            "dest": "/index.html"
        }
    ]
}
