{
    "version": 2,
    "public": false,
    "rewrites": [
        {
            "source": "/dist/server.js",
            "destination": "/api/server.js"
        }
    ],
    "routes": [
        {
            "src": "^/(.*)",
            "dest": "/$1"
        },
        {
            "src": ".*",
            "dest": "/index.html"
        }
    ]
}
