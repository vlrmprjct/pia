{
    "name": "pia",
    "version": "0.2.4a",
    "description": "PIA, (Electronic) Parts Inventory Assistant",
    "main": "src/server/index.js",
    "scripts": {
        "build": "rm -rf dist && webpack --config config/webpack.config.js --env.production",
        "build:start": "concurrently 'node dist/server.js' 'npx http-server dist'",
        "start": "concurrently 'yarn start:server' 'yarn start:client' 'nodemon dist/server.js'",
        "start:client": "webpack-dev-server --config config/webpack.config.js --config-name client --env.development --hot",
        "start:server": "webpack --config config/webpack.config.js --config-name server --env.development --profile --watch",
        "now-build": "yarn build"
    },
    "author": "Thomas Meschke",
    "license": "MIT",
    "dependencies": {
        "@csstools/normalize.css": "^11.0.1",
        "babel-polyfill": "^6.26.0",
        "better-sqlite3": "^5.4.3",
        "concurrently": "^6.0.2",
        "cuid": "^2.1.8",
        "date-and-time": "^2.0.1",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "gitrows": "^0.8.0",
        "match-sorter": "^4.0.2",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.13.0",
        "passport": "^0.4.1",
        "passport-github": "^1.1.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-router-dom": "^5.1.2",
        "react-splitter-layout": "^4.0.0",
        "react-table": "^7.0.0",
        "uikit": "^3.3.1",
        "webpack-stylish": "^0.1.8"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "@svgr/webpack": "^5.5.0",
        "babel-eslint": "^10.0.0",
        "babel-loader": "^8.0.0",
        "css-loader": "^2.0.0",
        "dotenv": "^8.2.0",
        "dotenv-flow": "^3.2.0",
        "dotenv-flow-webpack": "^1.1.0",
        "eslint": "^5.0.0",
        "eslint-config-airbnb": "^17.0.0",
        "eslint-plugin-import": "^2.11.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.7.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^3.0.0",
        "html-webpack-plugin": "^3.2.0",
        "nodemon": "^1.17.3",
        "sass-loader": "^8.0.0",
        "style-loader": "^0.23.0",
        "url-loader": "^1.0.1",
        "webpack": "^4.5.0",
        "webpack-cli": "^3.0.8",
        "webpack-dev-server": "^3.1.3"
    },
    "proxy": "http://localhost:5000/"
}
